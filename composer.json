{
  "name": "awaistech/larpack",
  "description": "AwaisAdmin is a powerful and easy-to-use Laravel package that provides a fully functional admin panel for managing blogs, SEO settings, and contact messages. It is designed to streamline content management while offering essential SEO tools and an intuitive contact message system.",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "spatie/laravel-activitylog": "^4.0",
    "illuminate/support": "^11.0 || ^10.0 || ^12.0 || ^13.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Awaistech\\Larpack\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Awais Safdar",
      "email": "awaissafdar111@gmail.com"
    }
  ],
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Awaistech\\Larpack\\PackageServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
        "@php artisan vendor:publish --tag=larpack-config",
        "@php artisan migrate"
    ]
},

    "minimum-stability": "dev",
    "prefer-stable": true
}
