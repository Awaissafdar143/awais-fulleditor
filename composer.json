{
  "name": "awaistech/larpack",
  "description": "AwaisAdmin is a powerful and easy-to-use Laravel package that provides a fully functional admin panel for managing blogs, SEO settings, and contact messages.",
  "type": "library",
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^11.0 || ^12.0 || ^13.0",
    "spatie/activitylog": "^4.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Awaistech\\Larpack\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Awais Safdar",
      "email": "awaissafdar111@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Awaistech\\Larpack\\PackageServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan vendor:publish --tag=larpack-config --force",
      "@php artisan migrate"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
